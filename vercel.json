{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "rewrites":  [
    {"source": "/(.*)", "destination": "/"}
  ],
  {
  "redirects": [
    {
      "source": "/menu",
      "destination": "https://drive.google.com/drive/u/1/folders/"
    }
  ]
}

}
